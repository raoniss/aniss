<%- include('../../inc/admin_header.ejs')  %>

<body>
    <div class="loader"></div>
    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            <div class="navbar-bg"></div>
            <%- include ('../../inc/admin_navbar') %>
            <div class="main-sidebar sidebar-style-2">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href="index.html"> <img alt="image" src="/assets/image/img/logo.png" class="header-logo" />
                        </a>
                    </div>
                    <div class="sidebar-user">

                        <div class="sidebar-user-details">
                            <div class="user-name">Sarah Smith</div>
                            <div class="user-role">Administrator</div>
                        </div>
                    </div>
                    <ul class="sidebar-menu">
                        <li class="menu-header">Main</li>
                        <li class="dropdown ">
                            <a href="#" class="nav-link has-dropdown"><i data-feather="users"></i>
                                <span>Utilisateurs</span></a>
                            <ul class="dropdown-menu">
                                <li class="active"><a class="nav-link" href="/admin/user">utilisateurs</a></li>
                                <li><a class="nav-link" href="/admin/user_proj">lanceur de projet</a></li>
                            </ul>
                        </li>
                        <li class="dropdown active">
                            <a href="#" class="nav-link has-dropdown"><i data-feather="briefcase"></i>
                                <span>Projet</span></a>
                            <ul class="dropdown-menu">
                                <li><a class="nav-link" href="/admin/projet_at">projet</a></li>
                                <li class="active"><a class="nav-link" href="#">campagne</a></li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="nav-link has-dropdown"><i data-feather="anchor"></i>
                                <span>Autre</span></a>
                            <ul class="dropdown-menu">
                                <li><a class="nav-link" href="/admin/categorie">Categorie</a></li>
                                <li><a class="nav-link" href="/admin/pays">Pays</a></li>
                            </ul>
                        </li>
                    </ul>
                </aside>
            </div>
            <!-- Main Content -->
            <div class="main-content">
                <section class="section">
                    <div class="section-body">
                        
                    </div>
                    <div class="row">
                        <div class="col-12 col-md-12 col-lg-12">
                            
                            <div class="card">
                                <div class="card-header">
                                    <h4>Liste des campagne</h4>
                                </div>
                                <div class="card-body">
                                    <div class="table-responsive">
                                        <table class="table filterTable table-bordered" data-filtering="true" data-sorting="true"
                                            data-paging="true" data-paging-size="5">
                                            <thead>
                                                <tr>
                                                    <th data-bs-toggle="true">Projet</th>
                                                    <th>Date de debut</th>
                                                    <th data-hide="all">Date de fin</th>
                                                    <th data-hide="all">Somme visee</th>
                                                    <th data-hide="all">Somme atteinte</th>
                                                    <th data-hide="all">Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% campagne.forEach(data => { %>
                                                    <tr>
                                                        <td data-hide="all"><%- data.Cp_projet %> </td>
                                                        <td data-hide="all"><%- data.Cp_date_debut %> </td>
                                                        <td data-hide="all"><%- data.Cp_date_fin %> </td>
                                                        <td data-hide="all"><%- data.Cp_montant_recuperes %> </td>
                                                        <td data-hide="all"><%- data.Cp_nombre_participants %> </td>
                                                        <td data-hide="all">
                                                            <a href="/campagne/voir/<%- data.idCampagne %>" class="btn btn-icon btn-primary"><i data-feather="eye"></i>
                                                            </a>

                                                            <a href="/campagne/message/<%- data.idCampagne %>" class="btn btn-icon btn-success "><i data-feather="mail"></i></a>
                                                        </td>
                                                    </tr>
                                                    <% }) %>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- add content here -->
            </div>
            </section>
            <%- include('../../inc/setting.ejs')  %>
            
        </div>
        <footer class="main-footer">
            <div class="footer-left">
                Copyright &copy; 2019 <div class="bullet"></div> Design By <a href="#">Redstar</a>
            </div>
            <div class="footer-right">
            </div>
        </footer>
    </div>
    </div>
    <!-- General JS Scripts -->
    <script src="/assets/temp/js/app.min.js"></script>
    <!-- JS Libraies -->
    <script src="/assets/temp/bundles/footable-bootstrap/js/footable.js"></script>
    <!-- Page Specific JS File -->
    <script src="/assets/temp/js/page/footable-data.js"></script>
    <!-- Template JS File -->
    <script src="/assets/temp/js/scripts.js"></script>
    <!-- Custom JS File -->
    <script src="/assets/temp/js/custom.js"></script>
</body>


